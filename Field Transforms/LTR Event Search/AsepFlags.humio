// Transform to convert AsepFlags decimal to human-readable value.
| AsepFlags match {
    1  => AsepFlags := "GLOBAL" ;
    2  => AsepFlags := "PRIVILEGED" ;
    3  => AsepFlags := "GLOBAL_PRIVILEGED" ;
    4  => AsepFlags := "NEEDS_SOURCE_SIGNED" ;
    8  => AsepFlags := "NEEDS_TARGET_SIGNED" ;
    16 => AsepFlags := "NEEDS_SOURCE_MS_SIGNED" ;
    32 => AsepFlags := "NEEDS_TARGET_MS_SIGNED" ;
    64 => AsepFlags := "SUSPICIOUS" ;
    *  => * }