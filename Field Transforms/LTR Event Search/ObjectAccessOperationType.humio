// Transform to convert ObjectAccessOperationType decimal to human-readable value.
| ObjectAccessOperationType match {
    1 => ObjectAccessOperationType := "CREATE" ;
    2 => ObjectAccessOperationType := "WRITE" ;
    3 => ObjectAccessOperationType := "DELETE" ;
    4 => ObjectAccessOperationType := "SET" ;
    5 => ObjectAccessOperationType := "RENAME" ;
    6 => ObjectAccessOperationType := "ATTRIBUTES_CHANGED" ;
    7 => ObjectAccessOperationType := "PERMISSIONS_CHANGED" ;
    * => * }