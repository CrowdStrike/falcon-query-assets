// Transform to convert CreateProcessType decimal to human-readable value.
| CreateProcessType match {
    0 => CreateProcessType := "Unavailable" ;
    1 => CreateProcessType := "Unknown" ;
    2 => CreateProcessType := "NTCreateProcess" ;
    3 => CreateProcessType := "CreateProcess" ;
    4 => CreateProcessType := "ShellExecute" ;
    5 => CreateProcessType := "CRT System" ;
    * => * }