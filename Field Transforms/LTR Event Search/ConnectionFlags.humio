// Transform to convert ConnectionFlags bitmask to human-readable value.

| ConnectionFlags match {
    1  => ConnectionFlags_RawSocket := "Yes";
    2  => ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    3  => ConnectionFlags_RawSocket := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    4  => ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes";
    5  => ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes" | ConnectionFlags_RawSocket := "Yes";
    6  => ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    7  => ConnectionFlags_RawSocket := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    8  => ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes";
    9  => ConnectionFlags_RawSocket := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes";
    10 => ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    11 => ConnectionFlags_RawSocket := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    12 => ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes";
    13 => ConnectionFlags_RawSocket := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes";
    14 => ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    15 => ConnectionFlags_RawSocket := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_Mcast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall_IGMPCast := "Yes" | ConnectionFlags_PromiscuousModeSioRcvall := "Yes";
    *  => * }