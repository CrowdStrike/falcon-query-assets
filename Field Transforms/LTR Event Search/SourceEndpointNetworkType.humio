// Transform to convert SourceEndpointNetworkType decimal to human-readable value.
| SourceEndpointNetworkType match {
    1  => SourceEndpointNetworkType := "INTERNAL" ;
    2  => SourceEndpointNetworkType := "VPN" ;
    4  => SourceEndpointNetworkType := "WIRELESS" ;
    8  => SourceEndpointNetworkType := "NAT" ;
    16 => SourceEndpointNetworkType := "PUBLIC" ;
    *  => * }