// Transform to convert ConnectionCipher decimal to human-readable value.
| ConnectionCipher match {
    26115 => ConnectionCipher := "CALG_3DES" ;
    26126 => ConnectionCipher := "CALG_AES_128" ;
    26128 => ConnectionCipher := "CALG_AES_256" ;
    26113 => ConnectionCipher := "CALG_DES" ;
    26114 => ConnectionCipher := "CALG_RC2" ;
    26625 => ConnectionCipher := "CALG_RC4" ;
    0     => ConnectionCipher := "N0_ENCRYPTION" ;
    *     => * }