// Transform to convert AuthenticationId decimal to human-readable value.
| AuthenticationId match {
    0    => AuthenticationId := "Invalid Luid" ;
    996  => AuthenticationId := "Network Service" ;
    997  => AuthenticationId := "Local Service" ;
    999  => AuthenticationId := "System" ;
    1000 => AuthenticationId := "Reserved Luid Max" ;
    *    => * }