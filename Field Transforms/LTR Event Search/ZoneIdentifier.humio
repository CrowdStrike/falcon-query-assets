// Transform to convert ZoneIdentifier decimal to human-readable value.
| ZoneIdentifier match {
    0 => ZoneIdentifier := "Local machine" ;
    1 => ZoneIdentifier := "Local intranet" ;
    2 => ZoneIdentifier := "Trusted sites" ;
    3 => ZoneIdentifier := "Internet" ;
    4 => ZoneIdentifier := "Restricted sites" ;
    * => * }