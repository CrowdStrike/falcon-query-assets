// Transform to convert KerberosAnomaly decimal to human-readable value.
| KerberosAnomaly match {
    1    => KerberosAnomaly := "MISSING_LOGON_SERVER_IN_REQUEST_TICKET" ;
    2    => KerberosAnomaly := "DEFAULT_PASSWORD_LAST_SET_VALUE_IN_REQUEST_TICKET" ;
    4    => KerberosAnomaly := "UNEXPECTED_SID_IN_REQUEST_TICKET" ;
    8    => KerberosAnomaly := "UNEXPECTED_PRIMARY_GROUP_ID_IN_REQUEST_TICKET" ;
    16   => KerberosAnomaly := "UNEXPECTED_GROUPS_ID_IN_REQUEST_TICKET" ;
    32   => KerberosAnomaly := "UNEXPECTED_RESOURCE_GROUPS_ID_IN_REQUEST_TICKET" ;
    64   => KerberosAnomaly := "UNEXPECTED_EXTRAS_SID_IN_REQUEST_TICKET" ;
    128  => KerberosAnomaly := "RC4_PAC" ;
    256  => KerberosAnomaly := "PACLESS_TICKET_GENERATION" ;
    512  => KerberosAnomaly := "AP_REQ_MISSING_AUTHENTICATOR_CHECKSUM" ;
    1024 => KerberosAnomaly := "AP_REQ_ZERO_FILLED_BIND_IN_AUTHENTICATOR_CHECKSUM" ;
    2048 => KerberosAnomaly := "UNEXPECTED_HOSTNAME_IN_SERVICE_ACCESS_REQUEST" ;
    *    => * }