// Transform to convert SourceEndpointHostNameResolutionMethod decimal to human-readable value.
| SourceEndpointHostNameResolutionMethod match {
    0 => SourceEndpointHostNameResolutionMethod := "RAW_PACKET_DATA" ;
    1 => SourceEndpointHostNameResolutionMethod := "TRAFFIC_CACHE" ;
    2 => SourceEndpointHostNameResolutionMethod := "KERBEROS_TICKET_CACHE" ;
    3 => SourceEndpointHostNameResolutionMethod := "REVERSE_LOOKUP" ;
    4 => SourceEndpointHostNameResolutionMethod := "FORWARD_LOOKUP" ;
    5 => SourceEndpointHostNameResolutionMethod := "NETBIOS_LOOKUP" ;
    6 => SourceEndpointHostNameResolutionMethod := "DOMAIN_CONTROLLER" ;
    * => * }