// Transform to convert MachOSubType decimal to human-readable value.
| MachOSubType match {
    0 => MachOSubType := "UNKNOWN" ;
    1 => MachOSubType := "EXECUTABLE64" ;
    2 => MachOSubType := "DYLIB64" ;
    3 => MachOSubType := "BUNDLE64" ;
    4 => MachOSubType := "KEXT64" ;
    5 => MachOSubType := "EXECUTABLE32" ;
    6 => MachOSubType := "DYLIB32" ;
    7 => MachOSubType := "BUNDLE32" ;
    * => * }