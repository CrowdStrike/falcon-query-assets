// Transform to convert NamedPipeImpersonationType decimal to human-readable value.
| NamedPipeImpersonationType match {
    0 => NamedPipeImpersonationType := "UNKOWN" ;
    1 => NamedPipeImpersonationType := "USER_TO_USER" ;
    2 => NamedPipeImpersonationType := "USER_TO_SERVICE" ;
    3 => NamedPipeImpersonationType := "USER_TO_SYSTEM" ;
    4 => NamedPipeImpersonationType := "SERVICE" ;
    * => * }