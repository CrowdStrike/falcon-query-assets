// Transform to convert ActiveDirectoryAuthenticationMethod decimal to human-readable value.
| ActiveDirectoryAuthenticationMethod match {
    0 => ActiveDirectoryAuthenticationMethod := "KERBEROS" ;
    1 => ActiveDirectoryAuthenticationMethod := "NTLM_V1" ;
    2 => ActiveDirectoryAuthenticationMethod := "NTLM_V2" ;
    3 => ActiveDirectoryAuthenticationMethod := "SIMPLE_BIND" ;
    4 => ActiveDirectoryAuthenticationMethod := "NO_AUTHENTICATION" ;
    5 => ActiveDirectoryAuthenticationMethod := "UNKNOWN_NTLM" ;
    * => * }