// Transform to convert ImageSubsystem decimal to human-readable value.
| ImageSubsystem match {
    0   => ImageSubsystem := "UNKNOWN" ;
    1   => ImageSubsystem := "NATIVE" ;
    2   => ImageSubsystem := "WINDOWS_GUI" ;
    3   => ImageSubsystem := "WINDOWS_CUI" ;
    7   => ImageSubsystem := "POSIX_CUI" ;
    256 => ImageSubsystem := "WSL" ;
    *   => * }