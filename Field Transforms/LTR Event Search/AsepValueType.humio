// Transform to convert AsepValueType decimal to human-readable value.
| AsepValueType match {
    0 => AsepValueType := "unknown" ;
    1 => AsepValueType := "cmd_line" ;
    2 => AsepValueType := "dll_path" ;
    3 => AsepValueType := "dll_name" ;
    4 => AsepValueType := "app_name" ;
    5 => AsepValueType := "guid" ;
    * => * }