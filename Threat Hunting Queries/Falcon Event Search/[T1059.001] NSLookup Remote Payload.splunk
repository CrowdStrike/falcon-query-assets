event_platform=win event_simpleName=ProcessRollup2 FileName=nslookup.exe ParentBaseFileName=powershell.exe
| regex CommandLine=".*\-q\=txt.*"
| stats values(UserName) as userName, values(ParentBaseFilName) as parentFile, values(FileName) as fileName, values(CommandLine) as cmdLine by cid, aid, ComputerName
